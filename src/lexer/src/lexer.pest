expr = { 
    select_stmt
}

keyword = _{
    select |
    from
}
    select = _{ ^"select" }
    from = _{ ^"from" }

name = { ASCII_ALPHANUMERIC+ }
    column_name = @{ name }
    table_name = @{ name }

WHITESPACE = _{ " " | "\t" }

select_stmt = { select ~ column_name ~ from ~ table_name }

expressions = { SOI ~ expr ~ EOI }
